if title
  a(href='#', class='add')
    span.title
      =title 
-var current = (list.length > 40 && jumper) ? ' ' : 'Z';
.category-list
  ul.category
    each item in list
      -if(item.Title.toUpperCase()[0] > current){
        -current= item.Title.toUpperCase()[0];
        li.jumper(id='jumper'+current)
      -}
      li(track-id=item._id) 
        = item.Title
